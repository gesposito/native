{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
      "key-spacing": 0,
      "quote-props": 0,
      "no-trailing-spaces": [2, { "skipBlankLines": true }],
      "padded-blocks": 0,
      "func-names": 0,

      "react/prefer-es6-class": 0,
      "react/prop-types": 0,
      "react/jsx-no-bind": [2, {
        "allowArrowFunctions": true
      }]
    }
}
